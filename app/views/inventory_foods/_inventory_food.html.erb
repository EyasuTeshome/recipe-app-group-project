<div id="<%= dom_id inventory_food %>">
  <p>
    <strong>Quantity:</strong>
    <%= inventory_food.quantity %>
  </p>

  <p>
    <strong>Food:</strong>
    <%= inventory_food.food_id %>
  </p>

  <p>
    <strong>Inventory:</strong>
    <%= inventory_food.inventory_id %>
  </p>
<table class="table">
  <thead>
    <tr>
      <th scope="col">Food</th>
      <th scope="col">Quantity</th>
      <th scope="col">Actions</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td><%= inventory_food.food_id %></td>
      <td>  <%= inventory_food.quantity %></td>
      <td> <%= inventory_food.inventory_id %></td>
    </tr>
  </tbody>
</table>
</div>
